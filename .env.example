# Optional: API key for Claude (not needed if using OAuth via Settings page)
# ANTHROPIC_API_KEY=sk-ant-...

# Optional: Git identity for commits made inside Docker sessions
# GIT_USER_NAME=Your Name
# GIT_USER_EMAIL=you@example.com

# Optional: GitHub PAT for git push from Docker sessions (use one, not both)
# GITHUB_TOKEN=ghp_xxxxxxxxxxxxxxxxxxxx
# GIT_CREDENTIALS_URL=https://username:token@github.com
